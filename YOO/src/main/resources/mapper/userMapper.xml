<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="week13.YOO.repository.UserMapper">
    <select id="getUserDB" parameterType="String" resultType="week13.YOO.domain.UserVO">
        select * from users where id = #{id};
    </select>
    <insert id="regiUserDB" parameterType="week13.YOO.dto.UserDto" >
        INSERT INTO users (id, pw, name) VALUES(#{id}, #{pw}, #{name});
    </insert>
</mapper>